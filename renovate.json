{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranchPatterns": [
    "main",
    "everything"
  ],
  "packageRules": [
    {
      "matchManagers": "github-actions",
      "matchBaseBranches": [
        "main"
      ]
    },
    {
      "matchManagers": "gradle",
      "matchBaseBranches": [
        "main"
      ]
    },
    {
      "matchManagers": "gradle-wrapper",
      "matchBaseBranches": [
        "main"
      ]
    },
    {
      "matchManagers": "maven",
      "matchBaseBranches": [
        "everything"
      ]
    },
    {
      "matchManagers": "maven-wrapper",
      "matchBaseBranches": [
        "everything"
      ]
    }
  ]
}
